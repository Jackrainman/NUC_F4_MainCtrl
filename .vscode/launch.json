{
    "version": "0.2.0",
    "configurations": [
        {
            "cwd": "${workspaceRoot}",
            "type": "cortex-debug",
            "request": "attach",
            "name": "Debug: OpenOCD",
            "servertype": "openocd",
            "executable": "build/Debug/F4-MainCtrl.elf",
            "runToEntryPoint": "main",
            "configFiles": [
                "interface/cmsis-dap.cfg",
                "target/stm32f4x.cfg"
            ],
            "svdFile": ".pack/Keil/STM32F4xx_DFP.2.14.0/CMSIS/SVD/STM32F429x.svd",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            }
        },
        {
            "cwd": "${workspaceRoot}",
            "type": "cortex-debug",
            "request": "attach",
           "name": "Debug: JLINK",
            "servertype": "jlink",
            "interface": "swd",
            "executable": "build/Debug/F4-MainCtrl.elf",
            "runToEntryPoint": "main",
            "device": "STM32F429VE",
            "svdFile": ".pack/Keil/STM32F4xx_DFP.2.14.0/CMSIS/SVD/STM32F429x.svd",
            "liveWatch": {
                "enabled": true,
                "samplesPerSecond": 4
            }
        }
    ]
}